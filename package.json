{
  "name": "wallet-persona-analyzer",
  "version": "1.0.0",
  "description": "A blockchain wallet analyzer generating detailed on-chain personas",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev:backend": "ts-node-dev --respawn src/server.ts",
    "dev:frontend": "cd wallet-persona-frontend && npm start",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build": "tsc && cd wallet-persona-frontend && npm run build"
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "wallet",
    "crypto",
    "web3"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@google/generative-ai": "^0.24.1",
    "@moralisweb3/common-evm-utils": "^2.25.0",
    "@moralisweb3/evm-api": "^2.25.0",
    "axios": "^1.9.0",
    "chart.js": "^4.4.9",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.10",
    "ethers": "^6.11.1",
    "express": "^4.18.2",
    "moralis": "^2.22.4",
    "morgan": "^1.10.0",
    "react-chartjs-2": "^5.3.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.5",
    "concurrently": "^8.2.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
